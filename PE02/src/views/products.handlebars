<div class="container">
    <h2>Listado de productos</h2>
    <div>
        {{#each docs}}
        <ul class="container fs-4">Code: <strong>{{this.code}}</strong>
            <li class="fs-5">Titulo: <strong>{{this.title}}</strong> - Descripcion: <strong>{{this.description}}</strong></li>
            <li class="fs-5">Precio: <strong>{{this.price}}</strong></li>
            <li class="fs-5">Stock: <strong>{{this.stock}}</strong></li>
            <li class="fs-5">Categor√≠a: <strong>{{this.category}}</strong></li>
            <li class="fs-5">Imagenes: <strong>{{this.thumbnails}}</strong></li>
        </ul>
        <br/>
        {{/each}}
    </div>
    <br/>
    <div>
        {{#if hasPrevPage}}
            <a href={{this.prevLink}}><< Anterior</a>
        {{/if}}
        &nbsp;
        {{this.page}}
        &nbsp;
        {{#if hasNextPage}}
            <a href={{this.nextLink}}>Siguiente >></a>
        {{/if}} 
    </div>
</div>